<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dialog_corner_bg"
    android:orientation="vertical"
    android:paddingTop="23dp">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:src="@mipmap/alipay_logo" />

        <ImageView
            android:id="@+id/dialog_recharge_cancel"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="22dp"
            android:background="@mipmap/gray_circle"
            android:gravity="center"
            android:scaleType="center"
            android:src="@mipmap/cha"
            android:textColor="@android:color/white"
            android:textSize="24sp" />
    </RelativeLayout>

    <TextView
        android:id="@+id/dialog_recharge_information"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:gravity="center"
        android:text="@string/account_information"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_marginTop="20dp"
        android:orientation="vertical"
        android:paddingBottom="40dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:src="@mipmap/dubi" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:orientation="vertical">


                        <TextView
                            android:id="@+id/dialog_recharge_dubi_8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="8.00"
                            android:textSize="16sp" />

                        <TextView
                            android:id="@+id/dialog_recharge_money_8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="¥8.00"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>

                <CheckBox
                    android:id="@+id/dialog_recharge_8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/selector_money_pay"
                    android:button="@null"
                    android:gravity="center"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:textColor="@color/selector_money_count_text"
                    android:textSize="16sp" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:src="@mipmap/dubi" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:orientation="vertical">


                        <TextView
                            android:id="@+id/dialog_recharge_dubi_18"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="18.00"
                            android:textSize="16sp" />

                        <TextView
                            android:id="@+id/dialog_recharge_money_18"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="¥18.00"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>

                <CheckBox
                    android:id="@+id/dialog_recharge_18"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/selector_money_pay"
                    android:button="@null"
                    android:gravity="center"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:textColor="@color/selector_money_count_text"
                    android:textSize="16sp" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:src="@mipmap/dubi" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:orientation="vertical">


                        <TextView
                            android:id="@+id/dialog_recharge_dubi_40"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="40.00"
                            android:textSize="16sp" />

                        <TextView
                            android:id="@+id/dialog_recharge_money_40"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="¥40.00"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>

                <CheckBox
                    android:id="@+id/dialog_recharge_40"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/selector_money_pay"
                    android:button="@null"
                    android:gravity="center"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:textColor="@color/selector_money_count_text"
                    android:textSize="16sp" />

            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:src="@mipmap/dubi" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:orientation="vertical">


                        <TextView
                            android:id="@+id/dialog_recharge_dubi_60"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="60.00"
                            android:textSize="16sp" />

                        <TextView
                            android:id="@+id/dialog_recharge_money_60"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="¥60.00"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>

                <CheckBox
                    android:id="@+id/dialog_recharge_60"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/selector_money_pay"
                    android:button="@null"
                    android:gravity="center"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:textColor="@color/selector_money_count_text"
                    android:textSize="16sp" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:src="@mipmap/dubi" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:orientation="vertical">


                        <TextView
                            android:id="@+id/dialog_recharge_dubi_98"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="98.00"
                            android:textSize="16sp" />

                        <TextView
                            android:id="@+id/dialog_recharge_money_98"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="¥98.00"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>

                <CheckBox
                    android:id="@+id/dialog_recharge_98"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/selector_money_pay"
                    android:button="@null"
                    android:gravity="center"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:textColor="@color/selector_money_count_text"
                    android:textSize="16sp" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="12dp"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:src="@mipmap/dubi" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:orientation="vertical">


                        <TextView
                            android:id="@+id/dialog_recharge_dubi_128"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="198.00"
                            android:textSize="16sp" />

                        <TextView
                            android:id="@+id/dialog_recharge_money_128"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="¥198.00"
                            android:textSize="12sp" />
                    </LinearLayout>
                </LinearLayout>

                <CheckBox
                    android:id="@+id/dialog_recharge_128"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/selector_money_pay"
                    android:button="@null"
                    android:gravity="center"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:textColor="@color/selector_money_count_text"
                    android:textSize="16sp" />

            </RelativeLayout>
        </LinearLayout>

        <TextView
            android:id="@+id/dialog_recharge_confirm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="40dp"
            android:background="@drawable/shape_login_button"
            android:paddingBottom="15dp"
            android:paddingLeft="110dp"
            android:paddingRight="110dp"
            android:paddingTop="11dp"
            android:text="@string/confirm_recharge"
            android:textColor="@color/blue"
            android:textSize="14sp" />

    </LinearLayout>


</LinearLayout>